<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home">

    <SearchView
        android:id="@+id/searchView"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:background="#D9D9D9"
        android:queryHint="Pesquisar"
        app:layout_constraintBottom_toTopOf="@+id/voltarPagamento"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="0dp"
        android:layout_height="94dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/rectangle_5" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="0dp"
        android:layout_height="52dp"
        android:layout_marginStart="48dp"
        android:backgroundTint="#00FFFFFF"
        android:contentDescription="bota a descrição aqui"
        app:layout_constraintStart_toEndOf="@+id/imageButton4"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:srcCompat="@drawable/carrinhoicon" />

    <ImageButton
        android:id="@+id/btnPerfil2"
        android:layout_width="55dp"
        android:layout_height="52dp"
        android:backgroundTint="#00FFFFFF"
        android:contentDescription="bota a descrição aqui"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.686"
        app:layout_constraintStart_toEndOf="@+id/imageButton"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:srcCompat="@drawable/variant3" />

    <ImageButton
        android:id="@+id/btnHome2"
        android:layout_width="0dp"
        android:layout_height="57dp"
        android:layout_marginStart="16dp"
        android:background="#00FFFFFF"
        android:contentDescription="bota a descrição aqui"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:srcCompat="@drawable/vector" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="Início"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnHome2" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="4dp"
        android:text="Pedido"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/imageButton4" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="4dp"
        android:text="Carrinho"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/textView20"
        app:layout_constraintTop_toBottomOf="@+id/imageButton" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Perfil"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.703"
        app:layout_constraintStart_toEndOf="@+id/textView21"
        app:layout_constraintTop_toBottomOf="@+id/btnPerfil2" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="56dp"
        android:layout_height="54dp"
        android:layout_marginStart="56dp"
        android:backgroundTint="#00FFFFFF"
        android:contentDescription="bota a descrição aqui"
        app:layout_constraintStart_toEndOf="@+id/btnHome2"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:srcCompat="@drawable/pedido" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Não perecíveis"
        app:layout_constraintBottom_toTopOf="@+id/imageButton17"
        tools:layout_editor_absoluteX="7dp" />

    <TextView
        android:id="@+id/emailVis"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Frango"
        app:layout_constraintBottom_toTopOf="@+id/voltarPagamento2"
        tools:layout_editor_absoluteX="184dp" />

    <TextView
        android:id="@+id/nomeVis"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:text="Carne suína"
        app:layout_constraintBottom_toTopOf="@+id/voltarPagamento2"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/editInfo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Carne bovina"
        app:layout_constraintBottom_toBottomOf="@+id/emailVis"
        tools:layout_editor_absoluteX="289dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Congelados"
        app:layout_constraintBottom_toTopOf="@+id/imageButton18"
        tools:layout_editor_absoluteX="169dp" />

    <ImageView
        android:id="@+id/voltarPagamento"
        android:layout_width="0dp"
        android:layout_height="131dp"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/propaganda" />

    <ImageView
        android:id="@+id/voltarPagamento2"
        android:layout_width="0dp"
        android:layout_height="131dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/propaganda" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Categorias"
        android:textColor="#80141E"
        android:textSize="23sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="228dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Legumes"
        app:layout_constraintBottom_toTopOf="@+id/imageButton19"
        tools:layout_editor_absoluteX="332dp" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="102dp"
        android:layout_height="86dp"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toStartOf="@+id/imageButton10"
        app:layout_constraintHorizontal_bias="0.224"
        app:layout_constraintStart_toEndOf="@+id/imageButton5"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.354"
        app:srcCompat="@drawable/rectangle_34" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="101dp"
        android:layout_height="89dp"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/rectangle_33" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="101dp"
        android:layout_height="89dp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/rectangle_35" />

    <ImageButton
        android:id="@+id/imageButton17"
        android:layout_width="104dp"
        android:layout_height="97dp"
        app:layout_constraintBottom_toTopOf="@+id/nomeVis"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/rectangle_36" />

    <ImageButton
        android:id="@+id/imageButton18"
        android:layout_width="104dp"
        android:layout_height="91dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toTopOf="@+id/emailVis"
        app:layout_constraintEnd_toStartOf="@+id/imageButton19"
        app:layout_constraintStart_toEndOf="@+id/imageButton17"
        app:srcCompat="@drawable/rectangle_37" />

    <ImageButton
        android:id="@+id/imageButton19"
        android:layout_width="100dp"
        android:layout_height="94dp"
        app:layout_constraintBottom_toTopOf="@+id/editInfo"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/rectangle_38" />


</androidx.constraintlayout.widget.ConstraintLayout>